module Io
  narrative: """Handles IO operations"""

outputs println(text String)
from
    println(text)

outputs print(text String)
from
    print(text)

inputs readln() String
from
    readln()

inputs read_file(path String) Result<String, Error>!
from
    read_file(path)

outputs write_file(path String, content String) Result<Unit, Error>!
from
    write_file(path, content)
