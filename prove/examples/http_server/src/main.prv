module Main
  narrative: """Defines route and handles routing"""
  Http transforms ok not_found

  type Route is Get(path String)
    | Post(path String)

transforms handle(route Route) String
from
    match route
        Get(path) => "GET " + path
        Post(path) => "POST " + path

main()
from
    println(handle(Get("/health")))
    println(handle(Post("/users")))
