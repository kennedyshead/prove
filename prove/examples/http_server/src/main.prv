with Http use transforms ok, transforms not_found

type Route is Get(path String)
    | Post(path String)

/// Route a request and return a response string.
/// handle(Get("/health")) == "GET /health"
/// handle(Post("/users")) == "POST /users"
transforms handle(route Route) String
from
    match route
        Get(path) => "GET " + path
        Post(path) => "POST " + path

main()
from
    println(handle(Get("/health")))
    println(handle(Post("/users")))
